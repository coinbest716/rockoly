{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars":[
      "error"
    ],
    "indent": "off",
    "linebreak-style":"off",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "object-shorthand":"off",
    "func-style":"off",
    "no-prototype-builtins":"off"
  }
}
